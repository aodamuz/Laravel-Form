{
    "name": "aodamuz/form",
    "description": "Generador de elementos de formulario de una manera f√°cil e-intuitiva.",
    "keywords": ["form", "input", "select", "checkbox", "radio", "textarea"],
    "type": "support",
    "license": "MIT",
    "authors": [ {
        "name": "Mauricio Mendoza",
        "email": "aodamuz@gmail.com"
    } ],
    "require": {
        "php": "^7.3|^8.0",
        "aodamuz/html": "^1.0"
    },
    "require-dev": {
        "codeception/assert-throws": "^1.1",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.19",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Aodamuz\\Form\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Aodamuz\\Form\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aodamuz\\Form\\FormFieldServiceProvider"
            ],
            "aliases": {
                "Input": "Aodamuz\\Form\\Facades\\Input"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
